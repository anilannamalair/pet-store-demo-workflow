export CONJUR_VERSION=5
export CONJUR_APPLIANCE_IMAGE=registry.tld/conjur-appliance:5.0-stable
export CONJUR_NAMESPACE_NAME=[namespace to deploy Conjur to]
export AUTHENTICATOR_ID=[unique authenticator ID for this deployment]
export CONJUR_FOLLOWER_COUNT=1
export OSHIFT_CONJUR_ADMIN_USERNAME=cadmin
export DEPLOY_MASTER_CLUSTER=true
export CONJUR_ACCOUNT=[Use this variable to set name of conjur account with which to authenticate to seed service]
export CONJUR_ADMIN_PASSWORD=[Use this variable to set conjur admin password]

